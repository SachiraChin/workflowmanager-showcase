{
  "type": "array",
  "display": "visible",
  "render_as": "card-stack",
  "items": {
    "type": "object",
    "display": "visible",
    "render_as": "card",
    "nudges": ["index-badge"],
    "selectable": true,
    "properties": {
      "text": {
        "type": "string",
        "display": true,
        "display_label": "Description"
      },
      "emojis": {
        "type": "string",
        "display": true,
        "display_label": "Emojis"
      }
    }
  }
}
