{
  "type": "array",
  "_ux.display": "visible",
  "_ux.render_as": "card-stack",
  "items": {
    "type": "object",
    "_ux": {
      "display": "visible",
      "render_as": "card",
      "nudges": ["index-badge"],
      "selectable": true
    },
    "properties": {
      "text": {
        "type": "string",
        "_ux.display": true,
        "_ux.display_label": "Description"
      },
      "emojis": {
        "type": "string",
        "_ux.display": true,
        "_ux.display_label": "Emojis"
      }
    }
  }
}
